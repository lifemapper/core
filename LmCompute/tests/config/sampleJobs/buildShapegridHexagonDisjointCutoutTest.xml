<lmTest>
   <!-- The name of the test -->
   <name>RAD Build ShapeGrid Hexagon with Disjoint Cutout Test</name>
   <!-- A description of what this test does -->
   <description>
      This test makes sure that the build shapegrid process fails with the 
      correct status when the cutout provided does not intersect with any cells
      in the shapegrid (hexagonal cells).
   </description>
   <testType>compute</testType>
   <!-- Is this test enabled -->
   <testLevel>20</testLevel>
   <!-- What is the expected job status result -->
   <expectedStatus>8610</expectedStatus>
   <jobXml>
      <!-- The job XML goes in this section, inside of a CDATA tag -->
      <![CDATA[
<Job>
   <jobId>basicShapegridHexagonDisjointCutoutTest</jobId>
   <processType>305</processType>
   <parentUrl>None</parentUrl>
   <url>http://sporks.nhm.ku.edu/services/rad/layers/1234</url>
   <userId>HuwPrice</userId>
   <minX>-30.0</minX>
   <minY>-30.0</minY>
   <maxX>30.0</maxX>
   <maxY>30.0</maxY>
   <cellSize>2.0</cellSize>
   <epsgCode>4326</epsgCode>
   <cellSides>6</cellSides>
   <cutoutWKT>POLYGON((-40 20, -40 10, -35 10, -40 20))</cutoutWKT>
</Job>
      ]]>
   </jobXml>
</lmTest>