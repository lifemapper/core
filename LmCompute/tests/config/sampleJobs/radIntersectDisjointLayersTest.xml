<lmTest>
   <!-- The name of the test -->
   <name>RAD Intersect Disjoint Layers</name>
   <!-- A description of what this test does -->
   <description>
      This test makes sure that RAD intersect doesn't fail when the layers 
      don't intersect.  Should just return a layer with no presences.
   </description>
   <!-- Is this test enabled -->
   <testLevel>20</testLevel>
   <!-- What is the expected job status result -->
   <expectedStatus>300</expectedStatus>
   <jobXml>
      <!-- The job XML goes in this section, inside of a CDATA tag -->
      <![CDATA[
<job>
   <processType>310</processType>
   <jobId>noIntersect</jobId>
   <parentUrl>None</parentUrl>
   <url>None</url>
   <userId>lm2</userId>
   <shapegrid>
      <url>http://LmComputeTest/test/shapegrid/shapefile</url>
      <localIdIndex>0</localIdIndex>
   </shapegrid>
   <layerSet>
      <layer>
         <index>0</index>
         <url>http://LmComputeTest/smallRadLayer/GTiff</url>
         <isRaster>True</isRaster>
         <resolution>1</resolution>
         <isOrganism>True</isOrganism>
         <attrPresence>pixel</attrPresence>
         <minPresence>3</minPresence>
         <maxPresence>8</maxPresence>
         <percentPresence>4</percentPresence>
         <attrAbsence />
         <minAbsence />
         <maxAbsence />
         <percentAbsence />
       </layer>
   </layerSet>
</job>
      ]]>
   </jobXml>
</lmTest>