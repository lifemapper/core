<lmTest>
   <!-- The name of the test -->
   <name>RAD Build ShapeGrid Hexagon (Large) Test</name>
   <!-- A description of what this test does -->
   <description>
      This is a test to make sure that the RAD build shapegrid process works 
      correctly with a large number of hexagonal cells.  This test attempts to 
      create a shapegrid with approximately 1 million cells.  That is larger 
      than our current usage.
   </description>
   <!-- Is this test enabled -->
   <testLevel>20</testLevel>
   <!-- What is the expected job status result -->
   <expectedStatus>300</expectedStatus>
   <jobXml>
      <!-- The job XML goes in this section, inside of a CDATA tag -->
      <![CDATA[
<Job>
   <jobId>basicShapegridHexagonLargeTest</jobId>
   <processType>305</processType>
   <parentUrl>None</parentUrl>
   <url>http://sporks.nhm.ku.edu/services/rad/layers/1234</url>
   <userId>HuwPrice</userId>
   <minX>-180.0</minX>
   <minY>-90.0</minY>
   <maxX>180.0</maxX>
   <maxY>90.0</maxY>
   <cellSize>.25</cellSize>
   <epsgCode>4326</epsgCode>
   <cellSides>6</cellSides>
</Job>
      ]]>
   </jobXml>
</lmTest>