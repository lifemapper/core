; .............................................................................
; Lifemapper configuration file for site-specific parameters 
; This file must be named or appended to site.ini, and installed in  
;   @LMHOME@/config to override variables in LIFEMAPPER_SERVER_CONFIG_FILE
; NOTE: For list variables, separate with commas, one line
; NOTE: This particular version is for badenov
; .............................................................................

[LmCommon - common]
ARCHIVE_USER:  pragma

[LmServer - environment]
DATASOURCE: PRAGMA

; ...............................................
; LmBackend-gt
; ...............................................
;; [LmBackend - SMTP]
;; SMTP_SERVER: smtp.ku.edu
;; SMTP_SENDER: lifemapper@ku.edu

; ...............................................
[LmServer - pipeline]
; ...............................................
SMTP_SERVER: smtp.ku.edu
SMTP_SENDER: lifemapper@ku.edu

TROUBLESHOOTERS: a.real.address@somewhere.com
SPECIES_EXP_YEAR: 2015
SPECIES_EXP_MONTH: 9
SPECIES_EXP_DAY: 27

; KU values
DEFAULT_ALGORITHMS: BIOCLIM,ATT_MAXENT
DEFAULT_MODEL_SCENARIO:  WC-10min
DEFAULT_PROJECTION_SCENARIOS: CCSM4-lgm-10min,CCSM4-mid-10min,CCSM4-RCP8.5-2070-10min,CCSM4-RCP4.5-2070-10min,CCSM4-RCP8.5-2050-10min,CCSM4-RCP4.5-2050-10min
SCENARIO_PACKAGE: 10min-past-present-future

